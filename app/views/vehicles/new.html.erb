<%= form_for @vehicle, url:{action:'create'} do |f| %>

<h1>Register your Vehicle</h1>

<form class="form" role="form">
  <div class="form-group">
     <%= f.label :model %> <br>
    <div class="col-lg-2">
      <%= f.text_field :model %>
    </div>
  </br>
  </div>
  <div class="form-group">
    <%= f.label :vin %> <br>
    <div class="col-lg-2">
      <%= f.text_field :vin %>
    </div>
  </div>
  </br>
  <div class="form-group">
    <%= f.label :year %> <br>
    <div class="col-lg-2">
      <%= f.number_field :year %>
    </div>
  </div>
    </br>
    </br>
  <div class="form-group" "actions">
    <%= f.submit "Register"%>
  </div>
  </br>
</div>
</form>
<% end %>
